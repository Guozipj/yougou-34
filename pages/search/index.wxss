.Input{
  display: flex;
  background-color:#eeeeee;
  padding:20rpx;
  justify-content: space-between;
}

.Input-background{
  display:flex;
  flex: 1;
  margin-right:20rpx;
  background-color:#fff;
  padding:10rpx 20rpx;
}
.conCel{
  width:150rpx;
  text-align: center;
  padding:3px;
  border: 1px solid #dddddd;
  border-radius:4px;

}

/* 历史记录 */
.hiStory{
  display:flex;
  justify-content: space-between;
  padding:20rpx;
}
.hiStory .hiStory-x{
  background-color:#dddddd;
  width:40rpx;
  height:40rpx;
  color:#fff;
  border-radius: 50%;
  text-align: center;
  line-height: 40rpx;
}
/* 搜索详情 */
.searchDetail{
  display:flex;
  flex-wrap: wrap;
}
.searchDetail view{
  background-color:#dddddd;
  padding:10rpx 18rpx;
  margin:10rpx 20rpx;
  font-size: 30rpx;
}